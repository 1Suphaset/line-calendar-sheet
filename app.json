{
  "name": "line-exercise-bot",
  "description": "LINE Exercise Bot with Google Calendar integration",
  "repository": "https://github.com/your-username/line-calendar-sheet",
  "keywords": ["line", "bot", "exercise", "calendar", "google"],
  "env": {
    "NODE_ENV": {
      "description": "Environment",
      "value": "production"
    },
    "PORT": {
      "description": "Port number",
      "value": "3000"
    },
    "LINE_CHANNEL_ACCESS_TOKEN": {
      "description": "LINE Bot Channel Access Token"
    },
    "LINE_CHANNEL_SECRET": {
      "description": "LINE Bot Channel Secret"
    },
    "GOOGLE_CLIENT_ID": {
      "description": "Google OAuth Client ID"
    },
    "GOOGLE_CLIENT_SECRET": {
      "description": "Google OAuth Client Secret"
    },
    "GOOGLE_REFRESH_TOKEN": {
      "description": "Google OAuth Refresh Token"
    },
    "SHEET_ID": {
      "description": "Google Sheets ID"
    }
  }
}
